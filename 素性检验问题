#include <stdio.h>
int n;
int m;
int main()
{
	scanf("%d", &n);
	if(n<=0)
	{
		printf("Nope..\n");
	}
	if(n==1)
	{
		printf("Nope..\n");
	}
	if(n==2)
	{
		printf("Yes!!\n");
	}
	else
	{
		for (m = 2; m < n; m++)
		{
			if(n % m == 0) 
			{
				printf("Nope..\n");
				break;
			}
		}
		if (m == n)
		{
			printf("Yes!!\n");
		}
	}
	return 0;
}
